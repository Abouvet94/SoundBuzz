<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}SoundBuzz{% endblock %}</title>
        {% block stylesheets %}
        {# Icone #}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" /> 
        {# Css #}
        <link rel="stylesheet" href="{{ asset('framework/css/style.css') }}" type="text/css" media="all"/>
        {# <link rel="stylesheet" href="{{ asset('framework/css/audioplayer.css') }}" type="text/css" media="all"/> #}
        <link rel="stylesheet" href="{{ asset('framework/css/player.css') }}" type="text/css" media="all"/>
            <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
        {# Scripts  #}
        <script type="text/javascript" src="{{ asset('framework/js/p5/p5.js') }}"></script>  
            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.6.1/addons/p5.dom.js"></script>
        <script type="text/javascript" src="{{ asset('framework/js/p5/p5.sound.js') }}"></script>            
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        <script type="text/javascript" src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
        <script type="text/javascript" src="{{ asset('framework/js/function.js') }}"></script>

        {# Script Player #}
        <script type="text/javascript" src="{{ asset('framework/js/player.js') }}"></script>
        {% endblock %}
    </body>
</html>
