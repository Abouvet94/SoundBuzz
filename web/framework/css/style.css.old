.formulaire .civi .radio {
  display: inline-block;
  font-size: 18px;
  line-height: 49px;
  cursor: pointer;
  text-align: left;
}
.formulaire .civi .radio:hover .inner {
  -moz-transform: scale(0.5);
  -o-transform: scale(0.5);
  -ms-transform: scale(0.5);
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  opacity: 0.5;
}
.formulaire .civi .radio input {
  height: 1px;
  width: 1px;
  opacity: 0;
}
.formulaire .civi .radio input:checked + .outer .inner {
  -moz-transform: scale(1);
  -o-transform: scale(1);
  -ms-transform: scale(1);
  -webkit-transform: scale(1);
  transform: scale(1);
  opacity: 1;
}
.formulaire .civi .radio input:checked + .outer {
  border: 3px solid #36b1ac;
}
.formulaire .civi .radio input:focus + .outer .inner {
  -moz-transform: scale(1);
  -o-transform: scale(1);
  -ms-transform: scale(1);
  -webkit-transform: scale(1);
  transform: scale(1);
  opacity: 1;
  background-color: #006f74;
}
.formulaire .civi .radio .outer {
  height: 20px;
  width: 20px;
  display: block;
  float: left;
  margin: 10px 9px 10px 10px;
  border: 3px solid #e0e0e0;
  border-radius: 50%;
  background-color: transparent;
}
.formulaire .civi .radio .outer .inner {
  -moz-transition: all 0.25s ease-in-out;
  -o-transition: all 0.25s ease-in-out;
  -webkit-transition: all 0.25s ease-in-out;
  transition: all 0.25s ease-in-out;
  height: 16px;
  width: 16px;
  -moz-transform: scale(0);
  -o-transform: scale(0);
  -ms-transform: scale(0);
  -webkit-transform: scale(0);
  transform: scale(0);
  display: block;
  margin: 2px;
  border-radius: 50%;
  background-color: #36b1ac;
  opacity: 0;
}
