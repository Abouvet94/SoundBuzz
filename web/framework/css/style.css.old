#container-menu {
  display: none;
}
#nav {
  background-color: rgba(26, 26, 26, 0.5);
  width: 100%;
  position: fixed;
  color: #fff;
  z-index: 2;
  transition: all ease 0.5s;
}
#nav .header {
  text-align: left;
  height: 80px;
  line-height: 80px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
#nav .header .logo {
  display: inline-block;
  width: 170px;
  height: 70px;
  font-weight: 800;
  background-image: url("/images/logo v2-1.png");
  background-size: 156px 60px;
  background-repeat: no-repeat;
  margin: 9px;
}
#nav .header .top-nav {
  position: absolute;
  right: 15px;
  display: inline-block;
  vertical-align: middle;
  width: auto !important;
}
@media screen and (max-width: 850px) {
  #nav .header .top-nav {
    display: none;
  }
}
#nav .header .top-nav li {
  display: inline-block;
  text-decoration: none;
  padding-left: 5px;
}
#nav .header .top-nav li:last-child {
  padding-left: 20px;
}
#nav .header .top-nav li a {
  color: #fff;
  text-decoration: none;
  font-weight: 300;
}
#nav .header .top-nav li .btn-border {
  text-align: center;
  display: inline-block;
  cursor: pointer;
  letter-spacing: 1px;
  position: relative;
  font-weight: 500;
  padding: 0 20px;
  margin: 10px 0;
  line-height: 40px;
  font-size: 15px;
}
#nav .header .top-nav li .btn-border:before {
  content: "";
  position: absolute;
  width: 0;
  background: #fff;
  left: 45%;
  height: 2px;
  bottom: 0;
  transition: all 0.3s;
  opacity: 0.7;
}
#nav .header .top-nav li .btn-border:hover:before {
  width: 100%;
  left: 0;
}
#nav .header .top-nav li .btn-log {
  display: inline-block;
  cursor: pointer;
  letter-spacing: 1px;
  position: relative;
  line-height: 40px;
  border-radius: 37px;
  background-color: transparent;
  -moz-transition: 0.6s;
  -o-transition: 0.6s;
  -webkit-transition: 0.6s;
  transition: 0.6s;
  padding: 0px 15px;
  font-weight: 500;
  font-size: 15px;
}
#nav .header .top-nav li .btn-log:hover {
  background: linear-gradient(to right, #f39865, #cc83ae, #7f74b4);
  color: #fff !important;
}
@media screen and (max-width: 850px) {
  #container-menu {
    width: 100%;
    height: 95%;
    position: fixed;
    color: #fff;
    z-index: 2;
  }
  #container-menu .navToggle {
    z-index: 4;
    position: absolute;
    width: 60px;
    height: 92px;
    top: -23px;
    right: -3px;
    margin: 0 auto;
    cursor: pointer;
  }
  #container-menu .navToggle .icon {
    -webkit-transition: all 0.4s ease-out;
    position: absolute;
    top: 60px;
    left: 0;
    margin: 0 auto;
    width: 30px;
    height: 3px;
    background-color: #fff;
  }
  #container-menu .navToggle .icon:before, #container-menu .navToggle .icon:after {
    -webkit-transition: all 0.25s ease-out;
    position: absolute;
    width: 30px;
    height: 3px;
    background-color: #fff;
    content: "";
  }
  #container-menu .navToggle .icon:before {
    top: -10px;
    left: 0;
  }
  #container-menu .navToggle .icon:after {
    top: 10px;
    left: 0;
  }
  #container-menu .navToggle:hover:before {
    top: -25px;
    background-color: #555;
  }
  #container-menu .navToggle:hover:after {
    top: 25px;
    background-color: #555;
  }
  #container-menu .navToggle.open .icon {
    -webkit-transition: all 0.4s ease-out;
    background-color: transparent;
    -webkit-transform: rotate(180deg);
  }
  #container-menu .navToggle.open .icon:before {
    top: 0;
    -webkit-transform: rotate(45deg);
  }
  #container-menu .navToggle.open .icon:after {
    top: 0;
    -webkit-transform: rotate(-45deg);
  }
  #container-menu #back-menu {
    display: flex;
    width: 100%;
    height: 100%;
    flex-direction: column;
    -ms-flex-pack: justify;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    -moz-justify-content: space-between;
    justify-content: space-around;
    position: absolute;
    top: 70px;
    z-index: 200;
  }
  #container-menu #back-menu.focus {
    background-color: #36b1ac;
    flex-direction: column;
    -ms-flex-pack: justify;
    -webkit-box-pack: justify;
    -webkit-transition: all 0.5s;
  }
  #container-menu #menu {
    z-index: 111;
    left: 0;
    right: 0;
    margin: 0 auto;
    position: absolute;
    width: 200px;
    height: 0px;
    overflow: hidden;
    text-align: center;
    border-bottom: 1px solid transparent;
    -webkit-transition: all 0.5s;
  }
  #container-menu #menu ul {
    list-style: none;
    padding: 0;
    font-family: sans-serif;
    font-size: 25px;
    font-weight: 300;
    display: flex;
    flex-direction: column;
  }
  #container-menu #menu li:first-child {
    margin-top: 0;
  }
  #container-menu #menu li:last-child {
    margin-top: 30px;
  }
  #container-menu #menu li {
    text-align: center;
    display: inline-block;
    cursor: pointer;
    letter-spacing: 1px;
    position: relative;
    padding: 0 20px;
    margin: 10px 0;
    line-height: 40px;
    margin: 15px 0;
    line-height: 20px;
    padding: 5px 0;
    cursor: pointer;
    text-decoration: none;
    -webkit-transition: all 0.25s;
  }
  #container-menu #menu li:before {
    content: "";
    position: absolute;
    width: 0;
    background: #fff;
    left: 45%;
    height: 2px;
    bottom: 0;
    transition: all 0.3s;
    opacity: 0.7;
  }
  #container-menu #menu li:before:hover:before {
    width: 100%;
    left: 0;
  }
  #container-menu #menu.active {
    height: 320px;
  }
  #container-menu #menu .btn-log {
    cursor: pointer;
    border-radius: 37px;
    border: solid 2px #fff;
    background-color: transparent;
    padding: 15px;
    -moz-transition: 0.6s;
    -o-transition: 0.6s;
    -webkit-transition: 0.6s;
    transition: 0.6s;
  }
  #container-menu #menu .btn-log:hover {
    background-color: #fff;
    border: solid 2px #fff;
    color: #36b1ac;
  }
}
